    <script>
	function tabs_z(o,o2,n,nm,cm){
			if(!tabs_z.nav){
				tabs_z.nav=[];
				var nav=document.getElementById('floatBar');
				var nav_div=nav.getElementsByTagName('div');
				var nav_div_l=nav_div.length;
				for(var i=0;i<nav_div_l;i++){
					if(nav_div[i].className.indexOf('nav_a')>=0){
						tabs_z.nav.push(nav_div[i]);
					}
				}
			}
            var m_n = document.getElementById(o).getElementsByTagName(nm);
            var a = document.getElementById(o2), b=a.getElementsByTagName(cm), c_n=[];
            for(var i=0; i<b.length; i++) if(b[i].parentNode==a) c_n.push(b[i]);
            for(i=0;i<m_n.length;i++){
                    m_n[i].className=i==n?"sel":"";
                    c_n[i].style.display=i==n?"block":"none";
					tabs_z.nav[i].style.display=i==n?"block":"none";
            }
    }
    
    function init_sel(id){
       var obj_link ="car_style_" + id;
       document.getElementById(obj_link).className="sel";
       document.getElementById(obj_link).href="javascript:void(0);";
       document.getElementById(obj_link).onclick="return false;";
       document.getElementById(obj_link).style.cursor="default";
       return true;
    }

    function select_me(id,curid,size){
           var obj_link ="car_style_" + id;
       document.getElementById(obj_link).className="sel";
       for(i=1; i<=size; i++){
              if(i!=id && i!=curid){
                        obj_link ="car_style_" + i;
            document.getElementById(obj_link).className="";
                  }
       }
       return true;
    }

    function disable_all(curid,size){
       var obj_link;
       for(i=1; i<=size; i++){
              if(i!=curid){
                        obj_link ="car_style_" + i;
            document.getElementById(obj_link).className="";
                  }
       }
       return true;
    }
   
    //init_sel(3);
    </script>
    
