<script type="text/javascript">
/**
 * 通过评论ID获取评论数
 * @name    getCount
 * @param   {String}    id 评论ID
 */
(function($){
    var Func = window['getCount'] = function(o){
        return new _getCount(o);
    },
    _getCount = function(o){
        this.pid = o.pid;
        this.dom = o.dom;
        this.init();
    }
    _getCount.prototype = {
        init: function(){
            var _this = this;
            $.getScript("http://coral.qq.com/article/batchcommentnum?targetid[]=" + _this.pid + "&source=1&callback=pinglunCount", function(){});
            window.pinglunCount = function(res){
                $("#" + _this.dom).html(res.data[0].commentnum);
            }
        }
    }
})(jQuery);

getCount({
    pid: "2645652256",
    dom: "pl-num"
});

</script>
